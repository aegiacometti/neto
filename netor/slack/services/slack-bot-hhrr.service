[Unit]
Description=Slack ChatOps Bot Service - Human Resources
After=network.target

[Service]
User=netor
Group=netor
EnvironmentFile=/etc/environment
Restart=always
RestartPreventExitStatus=SIGHUP
RestartSec=5
ExecStart=/usr/bin/python3 ${NETOR}netor/slack/slack-bot-hhrr.py

[Install]
WantedBy=default.target
